{
  "name": "html-sandbox",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "config": {
    "pathname": {
      "src": "src",
      "dist": "dist",
      "javascript": "assets/js",
      "publicPath": "/path/to/public/"
    },
    "extension": {
      "template": ".pug",
      "sass": ".sass, .scss",
      "javascript": ".ts, .js",
      "asset": ".jpg, .jpeg, png, .gif, .svg, .ico, .woff, .woff2",
      "resource": ".txt"
    }
  },
  "scripts": {
    "clean": "rm -rf $npm_package_config_pathname_dist node_modules yarn.lock yarn-error.log .eslintcache",
    "dev": "webpack-dev-server --open --useLocalIp --host 0.0.0.0 --port 3000",
    "lint:template": "prettier \"$npm_package_config_pathname_src/**/*?(.)$npm_package_config_extension_template\" --write",
    "lint:javascript": "eslint ./ --ext \"$npm_package_config_extension_javascript\" --fix --cache",
    "lint:typescript": "tsc",
    "test": "jest --config=.jestrc.js",
    "prebuild": "run-p test lint:*",
    "build": "NODE_ENV=production webpack"
  },
  "dependencies": {
    "core-js": "3.6.5"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-typescript": "7.10.1",
    "@prettier/plugin-pug": "1.4.4",
    "@testing-library/dom": "7.12.0",
    "@testing-library/jest-dom": "5.10.0",
    "@types/jest": "26.0.0",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "autoprefixer": "9.8.0",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.0.2",
    "css-loader": "3.5.3",
    "cssnano": "4.1.10",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jest": "23.13.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "fibers": "5.0.0",
    "file-loader": "6.0.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "glob": "7.1.6",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "imagemin": "7.0.1",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-jpegtran": "7.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-svgo": "8.0.0",
    "img-loader": "3.0.1",
    "jest": "26.0.1",
    "lint-staged": "10.2.10",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass-glob-importer": "5.3.2",
    "npm-run-all": "4.1.5",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "prettier": "2.0.5",
    "pug": "3.0.0",
    "pug-loader": "2.4.0",
    "raw-loader": "4.0.1",
    "sass": "1.26.8",
    "sass-loader": "8.0.2",
    "svg-inline-loader": "^0.8.2",
    "ts-jest": "26.1.0",
    "typescript": "3.9.5",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-fix-style-only-entries": "0.5.0"
  }
}
