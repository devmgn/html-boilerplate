{
  "name": "html-sandbox",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "os": [
    "!win32"
  ],
  "config": {
    "directory": {
      "dev": "_dev",
      "dest": "htdocs",
      "publicPath": "/",
      "css": "assets/css",
      "js": "assets/js",
      "images": "assets/images"
    },
    "fileExtension": {
      "pug": ".pug",
      "sass": ".sass, .scss",
      "js": ".ts, .js",
      "image": ".jpg, .jpeg, .png, .gif, .svg, .ico",
      "resource": ".txt, .pdf"
    }
  },
  "scripts": {
    "reinstall": "yarn clean && rm -rf $npm_package_config_directory_dest node_modules yarn.lock yarn-error.log && yarn",
    "clean": "rm -rf $npm_package_config_directory_dest",
    "dev": "yarn clean && tsc --watch & webpack-dev-server",
    "lint": "eslint test \"$npm_package_config_directory_dev\" --ext \"$npm_package_config_fileExtension_js\" --fix",
    "prebuild": "yarn clean && yarn lint && tsc",
    "build": "NODE_ENV=production webpack"
  },
  "dependencies": {
    "axios": "0.19.2",
    "body-scroll-lock": "2.6.4",
    "core-js": "3.6.4",
    "dom4": "2.1.5"
  },
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-typescript": "7.8.3",
    "@testing-library/dom": "7.0.2",
    "@testing-library/jest-dom": "5.1.1",
    "@types/jest": "25.1.4",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "autoprefixer": "9.7.4",
    "babel-loader": "8.0.6",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.2",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "file-loader": "5.1.0",
    "glob": "7.1.6",
    "html-webpack-plugin": "3.2.0",
    "imagemin": "7.0.1",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-jpegtran": "6.0.0",
    "imagemin-optipng": "7.1.0",
    "imagemin-svgo": "7.1.0",
    "img-loader": "3.0.1",
    "ip": "1.1.5",
    "jest": "25.1.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.1",
    "node-sass-glob-importer": "5.3.2",
    "postcss-flexbugs-fixes": "4.2.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.19.1",
    "pug": "2.0.4",
    "pug-loader": "2.4.0",
    "sass-loader": "8.0.2",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3",
    "url-loader": "3.0.0",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-fix-style-only-entries": "0.4.0"
  }
}
