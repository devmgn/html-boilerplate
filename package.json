{
  "name": "html-sandbox",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "os": [
    "!win32"
  ],
  "config": {
    "directory": {
      "src": "src",
      "dist": "dist",
      "publicPath": "",
      "css": "assets/css",
      "js": "assets/js",
      "images": "assets/images",
      "fonts": "assets/fonts"
    },
    "fileExtension": {
      "template": ".pug",
      "sass": ".sass, .scss",
      "js": ".ts, .js",
      "asset": ".jpg, .jpeg, png, .gif, .svg, .ico, .woff, .woff2",
      "resource": ".txt"
    }
  },
  "scripts": {
    "clean": "rm -rf $npm_package_config_directory_dist node_modules yarn.lock yarn-error.log .eslintcache",
    "dev": "webpack-dev-server --open --useLocalIp --host 0.0.0.0 --port 8000",
    "lint:template": "prettier \"$npm_package_config_directory_src/**/*?(.)$npm_package_config_fileExtension_template\" --write",
    "lint:js": "eslint ./ --ext \"$npm_package_config_fileExtension_js\" --fix --cache",
    "lint:typescript": "tsc",
    "test": "jest --config=.jestrc.js",
    "prebuild": "run-p test lint:*",
    "build": "NODE_ENV=production webpack"
  },
  "engines": {
    "node": ">13.0.0",
    "yarn": ">1.22.0"
  },
  "dependencies": {
    "axios": "0.19.2",
    "body-scroll-lock": "3.0.1",
    "core-js": "3.6.4",
    "dom4": "2.1.5",
    "intersection-observer": "0.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-typescript": "7.9.0",
    "@prettier/plugin-pug": "1.2.0",
    "@testing-library/dom": "7.1.1",
    "@testing-library/jest-dom": "5.2.0",
    "@types/jest": "25.1.4",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "autoprefixer": "9.7.5",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.2",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "file-loader": "6.0.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "glob": "7.1.6",
    "html-webpack-plugin": "4.0.1",
    "husky": "4.2.3",
    "imagemin": "7.0.1",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-jpegtran": "6.0.0",
    "imagemin-optipng": "7.1.0",
    "imagemin-svgo": "7.1.0",
    "img-loader": "3.0.1",
    "jest": "25.2.0",
    "lint-staged": "10.0.9",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.1",
    "node-sass-glob-importer": "5.3.2",
    "npm-run-all": "^4.1.5",
    "postcss-flexbugs-fixes": "4.2.0",
    "postcss-loader": "3.0.0",
    "prettier": "2.0.2",
    "pug": "2.0.4",
    "pug-loader": "2.4.0",
    "sass-loader": "8.0.2",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3",
    "url-loader": "4.0.0",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-fix-style-only-entries": "0.4.0"
  }
}
