    "pug-loader": "2.4.0",
    "sass": "1.32.0",
    "sass-loader": "10.1.0",
    "svg-sprite-loader": "5.2.1",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3",
    "webpack": "5.11.1",
    "webpack-cli": "4.3.1",
    "webpack-dev-server": "3.11.1",
    "webpack-remove-empty-scripts": "0.7.0"
  }
}
