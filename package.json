{
  "name": "html-sandbox",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "config": {
    "directory": {
      "src": "src",
      "dist": "dist",
      "javascript": "assets/js",
      "publicPath": "/path/to/public/"
    },
    "resolvedTarget": "\\.(ico|mp4|webm|woff2?)$",
    "copyTarget": "**/*.{txt,pdf}"
  },
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "clean": "rm -rf $npm_package_config_directory_dist node_modules yarn.lock yarn-error.log .eslintcache",
    "start": "webpack serve --useLocalIp --host 0.0.0.0 --port 3000 --progress",
    "lint:pug": "prettier \"$npm_package_config_directory_src/**/*.pug\" --write",
    "lint:javascript": "eslint ./ --ext .js,.jsx,.ts,.tsx --fix --cache",
    "lint:typescript": "tsc",
    "test": "jest --config=.jestrc.js",
    "prebuild": "run-p test lint:*",
    "build": "NODE_ENV=production webpack"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.pug": "yarn lint:pug",
    "*.js?(x)": "yarn lint:javascript",
    "*.ts?(x)": ["yarn lint:javascript", "yarn lint:typescript"]
  },
  "dependencies": {
    "core-js": "3.8.2",
    "dom4": "2.1.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@prettier/plugin-pug": "1.13.1",
    "@testing-library/dom": "7.29.1",
    "@testing-library/jest-dom": "5.11.8",
    "@types/copy-webpack-plugin": "6.4.0",
    "@types/dom4": "2.0.1",
    "@types/fibers": "3.1.0",
    "@types/friendly-errors-webpack-plugin": "0.1.2",
    "@types/imagemin-gifsicle": "7.0.0",
    "@types/imagemin-jpegtran": "5.0.0",
    "@types/imagemin-optipng": "5.2.0",
    "@types/imagemin-svgo": "8.0.0",
    "@types/imagemin-webp": "5.1.1",
    "@types/jest": "26.0.19",
    "@types/mini-css-extract-plugin": "1.2.2",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.7",
    "@types/sass": "1.16.0",
    "@types/svg-sprite-loader": "3.9.2",
    "@types/svgo": "1.3.3",
    "@types/webpack-dev-server": "3.11.1",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "autoprefixer": "10.2.0",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "7.0.0",
    "css-loader": "5.0.1",
    "cssnano": "4.1.10",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-standard": "4.1.0",
    "fibers": "5.0.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "glob": "7.1.6",
    "html-webpack-plugin": "4.5.1",
    "husky": "4.3.6",
    "imagemin": "7.0.1",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-jpegtran": "7.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-svgo": "8.0.0",
    "imagemin-webp": "6.0.0",
    "img-loader": "3.0.2",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "mini-css-extract-plugin": "1.3.3",
    "node-sass-glob-importer": "5.3.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.2.2",
    "postcss-flexbugs-fixes": "5.0.2",
