{
  "name": "html-sandbox",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "config": {
    "directory": {
      "src": "src",
      "dist": "dist",
      "javascript": "assets/js",
      "publicPath": "/path/to/public/"
    },
    "resourcesRegExp": "\\.(ico|mp4|webm|woff2?)$",
    "targetsPatternToCopy": "**/*.{txt,pdf}",
    "placeholders": "[name]-[contenthash]"
  },
  "scripts": {
    "prepare": "husky install",
    "preinstall": "npx only-allow yarn",
    "clean": "rm -rf $npm_package_config_directory_dist node_modules yarn.lock yarn-error.log .*lintcache",
    "start": "webpack serve --useLocalIp --host 0.0.0.0 --port 3000 --progress --open",
    "lint:pug": "prettier \"$npm_package_config_directory_src/**/*.pug\" --write",
    "lint:scss": "stylelint \"$npm_package_config_directory_src/**/*.scss\"  --fix --cache",
    "lint:javascript": "eslint ./ --ext .js,.jsx,.ts,.tsx --fix --cache",
    "lint": "yarn lint:pug & yarn lint:scss & yarn lint:javascript",
    "test": "jest --config=.jestrc.js",
    "prebuild": "yarn lint && yarn test",
    "build": "NODE_ENV=production webpack"
  },
  "lint-staged": {
    "*.pug": "yarn lint:pug",
    "*.scss": "yarn lint:scss",
    "*.[jt]s?(x)": "yarn lint:javascript"
  },
  "dependencies": {
    "@types/jquery": "^3.5.5",
    "core-js": "3.12.1",
    "dom4": "2.1.6",
    "jquery": "^3.6.0",
    "object-fit-images": "3.2.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/preset-env": "7.14.1",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "@prettier/plugin-pug": "1.15.2",
    "@testing-library/dom": "7.30.4",
    "@testing-library/jest-dom": "5.12.0",
    "@types/copy-webpack-plugin": "8.0.0",
    "@types/css-minimizer-webpack-plugin": "1.3.0",
    "@types/dom4": "2.0.1",
    "@types/fibers": "3.1.0",
    "@types/friendly-errors-webpack-plugin": "0.1.3",
    "@types/imagemin-gifsicle": "7.0.0",
    "@types/imagemin-mozjpeg": "8.0.0",
    "@types/imagemin-optipng": "5.2.0",
    "@types/imagemin-svgo": "9.0.0",
    "@types/imagemin-webp": "5.1.1",
    "@types/jest": "26.0.23",
    "@types/mini-css-extract-plugin": "1.4.3",
    "@types/object-fit-images": "3.2.0",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.3",
    "@types/react-router-dom": "5.1.7",
    "@types/sass": "1.16.0",
    "@types/svgo": "2.3.0",
    "@types/terser-webpack-plugin": "5.0.3",
    "@types/webpack-dev-server": "3.11.4",
    "@typescript-eslint/eslint-plugin": "4.22.1",
