{
  "name": "html-sandbox",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "config": {
    "directory": {
      "src": "src",
      "dist": "dist",
      "javascript": "assets/js",
      "publicPath": "/path/to/public/"
    },
    "extension": {
      "asset": ".ico, .mp4, .webm, .woff, .woff2",
      "resource": ".txt, .pdf"
    }
  },
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "clean": "rm -rf $npm_package_config_directory_dist node_modules yarn.lock yarn-error.log .eslintcache",
    "start": "webpack serve --useLocalIp --host 0.0.0.0 --port 3000 --progress",
    "lint:template": "prettier \"$npm_package_config_directory_src/**/*.pug\" --write",
    "lint:javascript": "eslint ./ --ext .js,.jsx,.ts,.tsx --fix --cache",
    "lint:typescript": "tsc",
    "test": "jest --config=.jestrc.js",
    "prebuild": "run-p test lint:*",
    "build": "NODE_ENV=production webpack"
  },
  "dependencies": {
    "core-js": "3.6.5",
    "dom4": "2.1.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@prettier/plugin-pug": "1.10.0",
    "@testing-library/dom": "7.26.3",
    "@testing-library/jest-dom": "5.11.5",
    "@types/dom4": "2.0.1",
    "@types/jest": "26.0.15",
    "@types/react": "16.9.55",
    "@types/react-dom": "16.9.9",
    "@types/react-router-dom": "5.1.6",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "autoprefixer": "10.0.1",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.2.1",
    "css-loader": "5.0.0",
    "cssnano": "4.1.10",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-standard": "15.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-standard": "4.0.2",
    "fibers": "5.0.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "glob": "7.1.6",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "imagemin": "7.0.1",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-jpegtran": "7.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-svgo": "8.0.0",
    "imagemin-webp": "6.0.0",
    "img-loader": "3.0.2",
    "jest": "26.6.1",
    "lint-staged": "10.5.0",
    "mini-css-extract-plugin": "1.2.1",
    "node-sass-glob-importer": "5.3.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.1.4",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "4.0.4",
    "postcss-sort-media-queries": "2.1.10",
    "prettier": "2.1.2",
    "pug": "3.0.0",
    "pug-loader": "2.4.0",
    "sass": "1.27.1",
    "sass-loader": "10.0.4",
    "svg-sprite-loader": "5.0.0",
    "ts-jest": "26.4.3",
    "typescript": "4.0.5",
    "webpack": "5.3.1",
    "webpack-cli": "4.1.0",
    "webpack-dev-server": "3.11.0",
    "webpack-fix-style-only-entries": "0.6.0"
  }
}
